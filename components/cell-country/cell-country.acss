.provider-select__body {
  display: flex;
  align-items: center;
  padding: 28rpx 0rpx 28rpx;
}

.provider-select__body .f-icon {
  flex-shrink: 0;
}

.provider-select__name {
  flex-shrink: 0;
  flex-grow: 1;
  margin: 0rpx 24rpx 0rpx;
}

.countryName{
  font-size: 16px;
  font-weight: bold;
}
.travel{
  font-size: 28rpx;
  color: #888;
  margin-top: 8rpx;
}

.provider-select__btn {
  flex-shrink: 1;
  width: initial;
}